<p id="notice"><%= notice %></p>

<h1>Listing Conta Pagars</h1>

<table>
  <thead>
    <tr>
      <th>Fornecedor</th>
      <th>Valor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conta_pagars.each do |conta_pagar| %>
      <tr>
        <td><%= conta_pagar.fornecedor %></td>
        <td><%= conta_pagar.valor %></td>
        <td><%= link_to 'Show', conta_pagar %></td>
        <td><%= link_to 'Edit', edit_conta_pagar_path(conta_pagar) %></td>
        <td><%= link_to 'Destroy', conta_pagar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Conta pagar', new_conta_pagar_path %>
