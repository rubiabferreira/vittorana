<p id="notice"><%= notice %></p>

<h1>Listing Fornecedors</h1>

<table>
  <thead>
    <tr>
      <th>Nomefantasia</th>
      <th>Razaosocial</th>
      <th>Endereco</th>
      <th>Bairro</th>
      <th>Cep</th>
      <th>Estado</th>
      <th>Cidade</th>
      <th>Tcomercial</th>
      <th>Tcelular</th>
      <th>Cnpj</th>
      <th>Ie</th>
      <th>Email</th>
      <th>Im</th>
      <th>Site</th>
      <th>Categoria</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fornecedors.each do |fornecedor| %>
      <tr>
        <td><%= fornecedor.nomefantasia %></td>
        <td><%= fornecedor.razaosocial %></td>
        <td><%= fornecedor.endereco %></td>
        <td><%= fornecedor.bairro %></td>
        <td><%= fornecedor.cep %></td>
        <td><%= fornecedor.estado %></td>
        <td><%= fornecedor.cidade %></td>
        <td><%= fornecedor.tcomercial %></td>
        <td><%= fornecedor.tcelular %></td>
        <td><%= fornecedor.cnpj %></td>
        <td><%= fornecedor.ie %></td>
        <td><%= fornecedor.email %></td>
        <td><%= fornecedor.im %></td>
        <td><%= fornecedor.site %></td>
        <td><%= fornecedor.categoria %></td>
        <td><%= link_to 'Show', fornecedor %></td>
        <td><%= link_to 'Edit', edit_fornecedor_path(fornecedor) %></td>
        <td><%= link_to 'Destroy', fornecedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fornecedor', new_fornecedor_path %>
