<h1>Listing clientes</h1>

<table>
  <thead>

    <tr>
      <th>Nome</th>
      <th>Conjuge</th>
      <th>Endereco</th>
      <th>Bairro</th>
      <th>Cep</th>
      <th>Estado</th>
      <th>Cidade</th>
      <th>Tresidencial</th>
      <th>Tcelular</th>
      <th>Cpf</th>
      <th>Rg</th>
      <th>Evento</th>
      <th>Horariofesta</th>
      <th>Duracaofesta</th>
      <th>Diasemana</th>
      <th>Cortesia</th>
      <th>Valorconsumacao</th>
      <th>Itensavulsos</th>
      <th>Dataevento</th>
      <th>Qtdecriancas</th>
      <th>Qtdeadultos</th>
      <th>Tema</th>
      <th>Gerarcontrato</th>
      <th>Valor</th>
      <th>Ndocumento</th>
      <th>Desconto</th>
      <th>Entrada</th>
      <th>Formapagamento</th>
      <th>Obs</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.nome %></td>
        <td><%= cliente.conjuge %></td>
        <td><%= cliente.endereco %></td>
        <td><%= cliente.bairro %></td>
        <td><%= cliente.cep %></td>
        <td><%= cliente.estado %></td>
        <td><%= cliente.cidade %></td>
        <td><%= cliente.tresidencial %></td>
        <td><%= cliente.tcelular %></td>
        <td><%= cliente.cpf %></td>
        <td><%= cliente.rg %></td>
        <td><%= cliente.evento %></td>

        <td><%= cliente.horarioFesta %></td>
        <td><%= cliente.duracaoFesta %></td>
        <td><%= cliente.diaSemana %></td>
        <td><%= cliente.cortesia %></td>
        <td><%= cliente.valorConsumacao %></td>
        <td><%= cliente.ItensAvulsos %></td>
        <td><%= cliente.dataEvento %></td>
        <td><%= cliente.qtdeCriancas %></td>
        <td><%= cliente.qtdeAdultos %></td>
        <td><%= cliente.tema %></td>
        <td><%= cliente.gerarContrato %></td>
        <td><%= cliente.valor %></td>
        <td><%= cliente.nDocumento %></td>
        <td><%= cliente.desconto %></td>
        <td><%= cliente.entrada %></td>
        <td><%= cliente.formaPagamento %></td>
        <td><%= cliente.obs %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
