<p id="notice"><%= notice %></p>

<h1>Listing Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Conjuge</th>
      <th>Endereco</th>
      <th>Bairro</th>
      <th>Cep</th>
      <th>Estado</th>
      <th>Cidade</th>
      <th>Tresidencial</th>
      <th>Tcelular</th>
      <th>Cpf</th>
      <th>Rg</th>
      <th>Evento</th>
      <th>Horario festa</th>
      <th>Duracao festa</th>
      <th>Dia semana</th>
      <th>Cortesia</th>
      <th>Valor consumacao</th>
      <th>Itens avulsos</th>
      <th>Data evento</th>
      <th>Qtde criancas</th>
      <th>Qtde adultos</th>
      <th>Tema</th>
      <th>Gerar contrato</th>
      <th>Valor</th>
      <th>Num documento</th>
      <th>Desconto</th>
      <th>Entrada</th>
      <th>Forma pagamento</th>
      <th>Obs</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.nome %></td>
        <td><%= cliente.conjuge %></td>
        <td><%= cliente.endereco %></td>
        <td><%= cliente.bairro %></td>
        <td><%= cliente.cep %></td>
        <td><%= cliente.estado %></td>
        <td><%= cliente.cidade%></td>
        <td><%= cliente.tresidencial %></td>
        <td><%= cliente.tcelular %></td>
        <td><%= cliente.cpf %></td>
        <td><%= cliente.rg %></td>
        <td><%= cliente.evento %></td>
        <td><%= cliente.horario_festa %></td>
        <td><%= cliente.duracao_festa %></td>
        <td><%= cliente.dia_semana %></td>
        <td><%= cliente.cortesia %></td>
        <td><%= cliente.valor_consumacao %></td>
        <td><%= cliente.itens_avulsos %></td>
        <td><%= cliente.data_evento %></td>
        <td><%= cliente.qtde_criancas %></td>
        <td><%= cliente.qtde_adultos %></td>
        <td><%= cliente.tema %></td>
        <td><%= cliente.gerar_contrato %></td>
        <td><%= cliente.valor %></td>
        <td><%= cliente.num_documento %></td>
        <td><%= cliente.desconto %></td>
        <td><%= cliente.entrada %></td>
        <td><%= cliente.forma_pagamento %></td>
        <td><%= cliente.obs %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
